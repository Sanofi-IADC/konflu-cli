{
  "name": "konflu-cli",
  "version": "1.0.0",
  "description": "Martial Arts for Confluence automation",
  "main": "./lib/index.js",
  "bin": {
    "konflu": "./lib/index.js"
  },
  "homepage": "https://github.com/Sanofi-IADC/konflu-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanofi-IADC/konflu-cli.git"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "rimraf ./lib && tsc -p .",
    "dev": "concurrently -k -n \"Typescript.Node\" -p \"[{name}]\" -c \"blue.green\" \"tsc --watch\" \"nodemon lib/index.js\"",
    "start:dev": "nodemon",
    "local": "sudo npm i -g && konflu",
    "refresh": "rimraf ./node_modules ./package-lock.json && npm install",
    "test:unit": "jest --verbose",
    "test:watch": "jest --verbose --watchAll",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "keywords": [
    "atlassian",
    "confluence",
    "CLI"
  ],
  "author": "Jose Gascon",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "commander": "^6.2.1",
    "configstore": "^5.0.1",
    "figlet": "^1.5.0",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "inquirer": "^7.3.3",
    "path": "^0.12.7",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/clui": "^0.3.0",
    "@types/configstore": "^4.0.0",
    "@types/figlet": "^1.2.1",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
